{
    "version": 2,
    "builds": [
        {
            "src": "vercel_app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/docs",
            "dest": "vercel_app.py"
        },
        {
            "src": "/openapi.json",
            "dest": "vercel_app.py"
        },
        {
            "src": "/api/v1/(.*)",
            "dest": "vercel_app.py"
        },
        {
            "src": "/(.*)",
            "dest": "vercel_app.py"
        }
    ],
    "env": {
        "ENVIRONMENT": "production",
        "LOG_LEVEL": "INFO",
        "OSRM_URL": "http://router.project-osrm.org/route/v1/foot",
        "NOMINATIM_URL": "https://nominatim.openstreetmap.org/search",
        "USER_AGENT": "SmartGlassesNavigationApp/1.0",
        "PYTHONPATH": ".",
        "PYTHONUNBUFFERED": "1"
    }
} 